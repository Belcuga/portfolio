{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|ico|svg|json|webmanifest|txt|woff2|woff))",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}